<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
           http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context.xsd
           http://www.springframework.org/schema/util
           http://www.springframework.org/schema/util/spring-util.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="com.fifth" />

    <util:list list-class="java.util.ArrayList" id="namesList">
        <value>RAM RAM</value>
        <value>JAI RAJA RAM</value>
        <value>RAM RAM</value>
        <value>SITA RAM</value>
    </util:list>
    
    <bean id="ob" class="com.fifth.beanScope">
     <constructor-arg ref="namesList"/>
	</bean>
	
    <bean id="ob1" class="com.fifth.beanScope" scope="prototype">
     <constructor-arg ref="namesList"/>
	</bean>
</beans>